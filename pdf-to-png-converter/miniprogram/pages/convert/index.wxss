.container { padding: 16rpx; }
.actions { display: flex; gap: 16rpx; margin-bottom: 16rpx; }
.tip { color: #888; font-size: 24rpx; margin: 8rpx 0; }
.grid { display: grid; grid-template-columns: 1fr 1fr; gap: 16rpx; }
.card { background: #fff; border-radius: 12rpx; padding: 12rpx; box-shadow: 0 2rpx 10rpx rgba(0,0,0,0.06); }
.meta { color: #666; font-size: 22rpx; margin-top: 8rpx; }


